    "description": "Database powered page builder for laravel livewire applications",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aashan\\LivewirePagebuilder\\": "src/",
            "Database\\Factories\\": "database/factories/"
        }
